{
    "name": "ehann/web-service-bundle",
    "description": "This bundle provides a way to automatically serialize and deserialize request parameters",
    "keywords": ["rest","controllers","doctrine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ethan Hann",
            "email": "ethanhann@gmail.com"
        }
    ],
    "require": {
        "sensio/framework-extra-bundle": "~3.0",
        "jms/serializer-bundle": "*",
        "willdurand/negotiation": "@stable"
    },
    "require-dev": {
        "symfony/browser-kit": "~2.4",
        "symfony/finder": "~2.4",
        "symfony/expression-language": "~2.4",
        "symfony/security-bundle": "~2.4"
    },
    "suggest": {
        "symfony/expression-language": "",
        "symfony/security-bundle": ""
    },
    "autoload": {
        "psr-0": { "Ehann\\Bundle\\WebServiceBundle": "" }
    },
    "target-dir": "Ehann/Bundle/WebServiceBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
